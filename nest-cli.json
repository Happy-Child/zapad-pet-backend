{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "webpack": true
  },
  "projects": {
    "pipes": {
      "type": "library",
      "root": "libs/pipes",
      "entryFile": "index",
      "sourceRoot": "libs/pipes/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pipes/tsconfig.lib.json"
      }
    },
    "filters": {
      "type": "library",
      "root": "libs/filters",
      "entryFile": "index",
      "sourceRoot": "libs/filters/src",
      "compilerOptions": {
        "tsConfigPath": "libs/filters/tsconfig.lib.json"
      }
    },
    "static-text": {
      "type": "library",
      "root": "libs/static-text",
      "entryFile": "index",
      "sourceRoot": "libs/static-text/src",
      "compilerOptions": {
        "tsConfigPath": "libs/static-text/tsconfig.lib.json"
      }
    },
    "pug-engine": {
      "type": "library",
      "root": "libs/pug-engine",
      "entryFile": "index",
      "sourceRoot": "libs/pug-engine/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pug-engine/tsconfig.lib.json"
      }
    },
    "pug-templates": {
      "type": "library",
      "root": "libs/pug-templates",
      "entryFile": "index",
      "sourceRoot": "libs/pug-templates/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pug-templates/tsconfig.lib.json"
      }
    }
  }
}