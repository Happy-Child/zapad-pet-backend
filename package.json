{
  "name": "zapad-pet",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "h:logs": "heroku logs --tail --app zapad-pet-backend",
    "h:release": "heroku container:release web --app zapad-pet-backend",
    "h:push": "heroku container:login && heroku container:push web --app zapad-pet-backend",
    "docker:build-image": "docker build . -t zapad-pet-backend",
    "docker:remove-images": "docker rmi -f zapad-pet-backend registry.heroku.com/zapad-pet-backend/web",
    "h:build": "yarn docker:remove-images && yarn docker:build-image && yarn h:push && yarn h:release",
    "dc:up": "docker-compose -f docker-compose.dev.yml up -d",
    "dc:down": "docker-compose -f docker-compose.dev.yml -v down"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/bcrypt": "^3.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/pug": "^2.0.4",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "config": "^3.3.3",
    "cookie-parser": "^1.4.5",
    "libphonenumber-js": "^1.9.7",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "pug": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/pipes/(.*)": "<rootDir>/libs/pipes/src/$1",
      "@libs/pipes": "<rootDir>/libs/pipes/src",
      "@libs/filters/(.*)": "<rootDir>/libs/filters/src/$1",
      "@libs/filters": "<rootDir>/libs/filters/src",
      "@libs/static-text/(.*)": "<rootDir>/libs/static-text/src/$1",
      "@libs/static-text": "<rootDir>/libs/static-text/src",
      "@libs/pug-engine/(.*)": "<rootDir>/libs/pug-engine/src/$1",
      "@libs/pug-engine": "<rootDir>/libs/pug-engine/src",
      "@libs/pug-templates/(.*)": "<rootDir>/libs/pug-templates/src/$1",
      "@libs/pug-templates": "<rootDir>/libs/pug-templates/src"
    }
  }
}
